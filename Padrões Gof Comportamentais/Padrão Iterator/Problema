/// Usado para coleçoes, armazenamento de objetos de tamanhos destintos
/// usa metodo iterator de collection
/// metodos
/// hasnext = tem alguem apos o objeto em questao q estou vendo na lista encadeada da coleçao?
/// next = pega o objeto seguinte
/// remove = remove um elemento
/// permite acesso sequencial de objetos de uma coleçao sem expor sua representacao/organizacao interna


public class Exemplo {
    public void imprimirProdutos(ArrayList<Produto> produtos) {
        for (Produto produto : produtos)
                System.out.printLn (produto.getNome());
        }
}
